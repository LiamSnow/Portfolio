{
"hosting": {
	"public": "src",
	"ignore": [
		"firebase.json",
		"**/.*",
		"**/node_modules/**"
	],
	"rewrites": [
		{
			"source": "/",
			"function": "getPage"
		},
		{
			"source": "/projects",
			"function": "getPage"
		},
		{
			"source": "/blog",
			"function": "getPage"
		},
		{
			"source": "/blog/*",
			"function": "getPage"
		}
	]
}
}
